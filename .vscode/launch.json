{
    "version": "0.2.0",
    "configurations": [
        {
            // require deno(https://deno.land/) + packup(https://packup.deno.dev/) installed
            "name": "Packup Build",
            "request": "launch",
            "type": "pwa-node",
            "program": "src/index.html",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "packup",
            "runtimeArgs": [
                "build"
            ]
        }
    ]
}